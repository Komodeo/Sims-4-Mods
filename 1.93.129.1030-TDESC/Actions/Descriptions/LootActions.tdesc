                        <TunableList name="multipliers" class="TunableList" display="Multipliers" muid="875371C49F55338E" description="A list of multipliers to apply to base_chance." Deprecated="False" filter="0" group="General">
                            <TunableTuple class="TunableTuple" muid="1AC881A3C48767A0" Deprecated="False" filter="0" group="General">
                                <Tunable type="float" name="multiplier" class="TunableRange" default="1" display="Multiplier" muid="D31593E99ECA87AF" min="0" max="None" description="The multiplier to apply to base_chance if the associated&#xA;tests pass." tuning_state="NeedsTuning" Deprecated="False" filter="0" group="General" />
