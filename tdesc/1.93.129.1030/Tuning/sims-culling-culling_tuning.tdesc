            <Tunable type="float" name="CULLING_GHOST_WARNING_THRESHOLD" class="Tunable" default="0" display="Culling Ghost Warning Threshold" muid="7DCD577C41D70AC5" description="Once a ghost's culling commodity is below this threshold, a warning&#xA;notification is displayed." tuning_state="NeedsTuning" Deprecated="False" filter="0" group="General" />

